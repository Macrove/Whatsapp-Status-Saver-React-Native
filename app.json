{
  "expo": {
    "name": "Status Saver for WhatsApp",
    "slug": "whatsapp-status-saver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.MacroveBuilds.WhatsAppStatusSaver",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FILE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Please allow ${PRODUCT_NAME} to access your photos.",
          "savePhotosPermission": "Please allow ${PRODUCT_NAME} to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ],
      [
        "expo-file-system",
        {
          "READ_EXTERNAL_STORAGE_PERMISSION": "Please allow ${PRODUCT_NAME} to access your external storage",
          "WRITE_EXTERNAL_STORAGE_PERMISSION": "Please allow ${PRODUCT_NAME} to access your external storage"
        }
      ]
    ]
  }
}
